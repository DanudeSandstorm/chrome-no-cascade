{
	"manifest_version": 2,

	"name": "Chrome no Cascade",
	"description": "Prevents chrome from cascading new windows.",
	"version": "0.4",

	"icons": {
		"16": "chrome-cascade.png"
	},

	"permissions": [
		"activeTab"
	],

	"background": {
		"scripts": ["background.js"]
	},

	"browser_action": {
		"default_icon": "chrome-cascade.png"
	}

}
